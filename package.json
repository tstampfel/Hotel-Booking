{
  "name": "sparkli-node",
  "version": "1.0.0",
  "description": "Sparkli Nodejs & ReactJS",
  "main": "app.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "server": "env-cmd -f ./config/dev.env ts-node-dev --respawn src/index.ts",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "watch-and-run": "env-cmd -f ./config/dev.env tsc -w & nodemon -q -w dist dist/index.js",
    "test": "env-cmd -f ./config/test.env jest --watch --runInBand",
    "debug": "env-cmd -f ./config/dev.env ts-node-dev --respawn --prefer-ts --debug --inspect -- src/index.ts",
    "client:build": "cd client && npm run build",
    "client": "cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run client\" \"npm run server\"",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://tomislavstampfel@dev.azure.com/tomislavstampfel/Sparkli-Node/_git/Sparkli-Node"
  },
  "author": "Tomislav Stampfel",
  "license": "ISC",
  "dependencies": {
    "@types/graphql": "^14.0.4",
    "@types/ioredis": "^4.16.1",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.12.18",
    "@types/nodemailer": "^4.6.5",
    "@types/uuid": "^3.4.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "0.0.8",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "apollo-server": "^2.13.0",
    "apollo-server-express": "^2.3.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.3",
    "class-validator": "^0.9.1",
    "concurrently": "^5.3.0",
    "connect-redis": "^3.4.2",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "graphql": "^14.0.2",
    "graphql-query-complexity": "^0.2.2",
    "graphql-redis-subscriptions": "^2.2.1",
    "graphql-subscriptions": "^1.1.0",
    "helmet": "^4.4.1",
    "ioredis": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "nodemailer": "^5.1.1",
    "pg": "^8.4.0",
    "react-bootstrap": "^1.4.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.6",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.29",
    "uuid": "^3.3.2",
    "typescript": "^4.1.2",
    "ts-node": "^9.1.1",
    "nodemon": "^1.18.9",
    "ts-node-dev": "^1.1.1"
  },
  "devDependencies": {
    "@types/faker": "^4.1.4",
    "env-cmd": "^10.1.0",
    "faker": "^4.1.0",
    "jest": "26.6.0",
    "ts-jest": "^26.4.4"
  }
}
